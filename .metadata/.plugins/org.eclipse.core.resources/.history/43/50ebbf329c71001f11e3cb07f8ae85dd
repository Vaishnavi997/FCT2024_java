// Class to represent the preparation of the lesson
class LessonPreparation {
    String lessonPlan;
    String teachingMaterials;

    void prepareLesson() {
        // Code to prepare the lesson plan and gather teaching materials
    }
}

// Class to represent the delivery of the lesson
class LessonDelivery {
    String classroom;
    String whiteboard;

    void deliverLesson() {
        // Code to deliver the lesson in the classroom using the whiteboard
    }
}

// Class to represent the assessment of students
class StudentAssessment {
    String test;
    String gradingCriteria;

    void assessStudents() {
        // Code to give a test to students and assess their performance
    }
}

// Class to represent giving feedback to students
class Feedback {
    String feedbackForm;

    void provideFeedback() {
        // Code to provide feedback to students based on their performance
    }
}

// Example usage
public class TeachingProcess {
    public static void main(String[] args) {
        // Prepare the lesson
        LessonPreparation prep = new LessonPreparation();
        prep.prepareLesson();

        // Deliver the lesson
        LessonDelivery deliver = new LessonDelivery();
        deliver.deliverLesson();

        // Assess students
        StudentAssessment assess = new StudentAssessment();
        assess.assessStudents();

        // Provide feedback
        Feedback feedback = new Feedback();
        feedback.provideFeedback();
    }
}
